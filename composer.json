        "issues": "https://github.com/shashenko/omnipay-yookassa/issues",
        "wiki": "https://github.com/shashenko/omnipay-yookassa/wiki"
    },
    "authors": [
        {
            "name": "Shashenko Andrey",
            "role": "Developer"
        },
        {
            "name": "Dmitriy Smirnov",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6",
        "ext-json": "*",
        "omnipay/common": "~2.0",
        "yoomoney/yookassa-sdk-php": "^2.0"
    },
    "require-dev": {
