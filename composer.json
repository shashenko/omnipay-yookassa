{
    "name": "shashenko/omnipay-yookassa",
    "type": "library",
    "description": "YooKassa driver for Omnipay payment processing library",
    "keywords": [
        "php",
        "omnipay",
        "yookassa",
        "payment"
    ],
    "homepage": "https://github.com/shashenko/omnipay-yookassa",
    "license": "MIT",
    "support": {
        "email": "saa@albatrosscom.ru",
